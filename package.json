{
  "name": "clipboard-sync-share-app",
  "version": "0.0.1-beta.1",
  "description": "可实时跨设备同步剪贴板的应用程序",
  "author": "xlboy",
  "main": "dist/electron/bootstrap.js",
  "packageManager": ">=pnpm@7.5.0",
  "engines": {
    "node": ">=17.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=dev vite",
    "build": "pnpm type-check && vite build && electron-builder -c ./src/electron/build/builder-config.js",
    "type-check": "tsx ./scripts/type-check.ts",
    "lint": "pnpm type-check && lint-staged",
    "clean": "rimraf ./node_modules",
    "postinstall": "simple-git-hooks"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/types": "^17.0.0",
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.1.4",
    "@mantine/hooks": "^5.1.4",
    "@mimi-utils/types": "^0.0.3",
    "@types/lodash": "^4.14.182",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "ahooks": "^3.7.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "electron": "^19.0.3",
    "electron-builder": "^23.0.3",
    "electron-store": "^8.0.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "execa": "^6.1.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "react-tracked": "^1.7.9",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "ts-toolbelt": "^9.6.0",
    "tsx": "^3.8.1",
    "twind": "^0.16.17",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-electron": "^0.6.1",
    "vitest": "^0.16.0",
    "zustand": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts, js}": [
      "eslint --cache --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm type-check && pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e $1 -g ./commitlint.config.js"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "0.0.0.0",
    "VITE_DEV_SERVER_PORT": 7777
  }
}
